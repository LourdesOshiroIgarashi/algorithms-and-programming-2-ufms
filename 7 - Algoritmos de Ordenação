// PROGRAMA PARA CONTAR O NUMERO DE INVERSOES EM UM VETOR ==> custo deste programa Ã©
// O(n^2)

// (I < J) E (V[I] > V[J]) --> INVERSAO
#include<stdio.h>
#define MAX 10


int main()
{
	int vetor[MAX];
	int i, j;
	int cont = 0;
	int n;

	scanf("%d", &n);
	for(i = 0; i < n; i++)
	{	
		scanf("%d", &vetor[i]);
	}

	// contando as INVERSOES
	for(i = 0; i < n-1; i++)
	{
		for(j = i+1; j < n; j++)
		{
			if(vetor[i] > vetor[j])
				cont++;
		}
	}

	printf("Inversoes: %d\n", cont);

}
